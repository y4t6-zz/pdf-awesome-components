mixin githubStar(extra_classes)
  - var default_classes = 'c-githubStar'
  if extra_classes
      - var classes = default_classes + ' ' + extra_classes
  else
      - var classes = default_classes

  a(class=classes, href='https://github.com/y4t6/pdf-awesome-components')
    img(src='/img/design/github-star.png', alt='github star logo')

             
